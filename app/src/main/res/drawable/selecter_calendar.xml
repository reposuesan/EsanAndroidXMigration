<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_selected="true" android:drawable="@drawable/bg_calendar_selected"/>
    <item android:state_pressed="true" android:drawable="@drawable/bg_calendar_selected"/>
    <item app:tsquare_state_current_month="false">
        <shape android:shape="rectangle">
            <solid android:color="#b7b7b7"/>
        </shape>
    </item>
    <item app:tsquare_state_today="true" android:drawable="@drawable/bg_calendar_hoy"/>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#f7f7f7"/>
        </shape>
    </item>
</selector>